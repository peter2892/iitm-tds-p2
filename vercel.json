{
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "rewrites": [
        { 
            "source": "/(.*)",
            "destination": "/" 
        }
    ]
}
